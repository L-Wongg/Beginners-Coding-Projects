# Mini Project - Random Password Generator!

# Importing relevant modules
from random import randint

# ALL uppercase password
password = ""
for i in range(10):
    i = chr(randint(65,90))   # .lower to generate a lowercase password
    password = str(password) + i
print(password)

# Upper and lowercase password
password = ""
for i in range(5):            # Counts for the number of uppercase
    i = chr(randint(65,90))
    for j in range (5):                # Counts for number of lowercase
        j = chr(randint(65,90)).lower()
    password = str(password) + i + j
print(password)
